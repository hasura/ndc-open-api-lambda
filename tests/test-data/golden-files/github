 * @allowrelaxedtypes
 * @readonly
 */
export async function getReposActionsGetActionsCacheList(
  owner: string,
  repo: string,
  query: {
    /** The number of results per page (max 100). */
    per_page?: number;
    /** Page number of the results to fetch. */
    page?: number;
