  owner: string,
  repo: string,
  pullNumber: number,
  /** Request body */
  data: {
    /** IP for location auto-detection when proxying a request */
    client_ip?: string;
    /** Path to devcontainer.json config to use for this codespace */
    devcontainer_path?: string;
