/**
 * undefined
 * @request GET :/#Action=AttachInstances
 * @allowrelaxedtypes
 * @readonly
 */
export async function getActionAttachInstancesGetAttachInstances(
  query: {
    /**
     * The IDs of the instances. You can specify up to 20 instances.
     */
    InstanceIds?: XmlStringMaxLen19[];
    /**
     * The name of the Auto Scaling group.
     */
    AutoScalingGroupName: string;
    Action: "AttachInstances";
    Version: "2011-01-01";
  },
  headers?: hasuraSdk.JSONValue,
): Promise<hasuraSdk.JSONValue> {
  const result = await api.actionAttachInstances.getAttachInstances({
    query: query,
    params: {
      headers: (headers?.value as Record<string, string>) ?? undefined,
    },
  });
  if (result.data) {
    return result.data;
  } else {
    throw result.error;
  }
}

