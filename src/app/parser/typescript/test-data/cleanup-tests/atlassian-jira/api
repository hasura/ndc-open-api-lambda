      query,
      params = {},
    }: {
      query?: {
        /**
         * The index of the first item to return in a page of results (page offset).
         * @default "0"
         */
        startAt?: string;
        /**
         * The maximum number of items to return per page.
         * @default "50"
         */
        maxResults?: string;
        /**
         * The list of notifications scheme IDs to be filtered out
         * @uniqueItems true
         */
