  type: string;
}

/** The default value for a User Picker (multiple) custom field. */
export interface CustomFieldContextDefaultValueMultiUserPicker {
  /** The IDs of the default users. */
  accountIds: string[];
  /** The ID of the context. */
  contextId: string;
  type: string;
}

