     * @tags Users
     * @name RemoveUser
     * @summary Delete user
     * @request DELETE:/rest/api/3/user
     * @secure
     */
    removeUser: ({
      query,
      params = {},
    }: {
      query: {
        /**
         * The account ID of the user, which uniquely identifies the user across all Atlassian products. For example, *5b10ac8d82e05b22cc7d4ef5*.
         * @maxLength 128
         * @example "5b10ac8d82e05b22cc7d4ef5"
         */
        accountId: string;
